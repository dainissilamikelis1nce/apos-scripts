    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "LOCAL_ENV",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.mjs",
            "envFile": "${workspaceFolder}/_envs/.env.local",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "PROD_ENV",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.mjs",
            "envFile": "${workspaceFolder}/_envs/.env.prod",
        }
    ]
}